{
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:import/errors",
    "prettier"
  ],
  "rules": {
    "import/resolver": true
  },
  "plugins": ["import"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "product_json": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
